<?xml version="1.0" encoding="UTF-8"?>
<project name="module_spring-boot-hello-master" default="compile.module.spring-boot-hello-master">
  <dirname property="module.spring-boot-hello-master.basedir" file="${ant.file.module_spring-boot-hello-master}"/>
  
  <property name="module.jdk.home.spring-boot-hello-master" value="${project.jdk.home}"/>
  <property name="module.jdk.bin.spring-boot-hello-master" value="${project.jdk.bin}"/>
  <property name="module.jdk.classpath.spring-boot-hello-master" value="${project.jdk.classpath}"/>
  
  <property name="compiler.args.spring-boot-hello-master" value="-encoding UTF-8 -source 8 -target 8 ${compiler.args}"/>
  
  <property name="spring-boot-hello-master.output.dir" value="undefined"/>
  <property name="spring-boot-hello-master.testoutput.dir" value="undefined"/>
  
  <path id="spring-boot-hello-master.module.bootclasspath">
    <!-- Paths to be included in compilation bootclasspath -->
  </path>
  
  <path id="spring-boot-hello-master.module.production.classpath"/>
  
  <path id="spring-boot-hello-master.runtime.production.module.classpath"/>
  
  <path id="spring-boot-hello-master.module.classpath"/>
  
  <path id="spring-boot-hello-master.runtime.module.classpath"/>
  
  
  <patternset id="excluded.from.module.spring-boot-hello-master">
    <patternset refid="ignored.files"/>
  </patternset>
  
  <patternset id="excluded.from.compilation.spring-boot-hello-master">
    <patternset refid="excluded.from.module.spring-boot-hello-master"/>
  </patternset>
  
  
  <target name="compile.module.spring-boot-hello-master" depends="compile.module.spring-boot-hello-master.production,compile.module.spring-boot-hello-master.tests" description="Compile module spring-boot-hello-master"/>
  
  <target name="compile.module.spring-boot-hello-master.production" depends="register.custom.compilers" description="Compile module spring-boot-hello-master; production classes"/>
  
  <target name="compile.module.spring-boot-hello-master.tests" depends="register.custom.compilers,compile.module.spring-boot-hello-master.production" description="compile module spring-boot-hello-master; test classes" unless="skip.tests"/>
  
  <target name="clean.module.spring-boot-hello-master" description="cleanup module">
    <delete dir="${spring-boot-hello-master.output.dir}"/>
    <delete dir="${spring-boot-hello-master.testoutput.dir}"/>
  </target>
</project>